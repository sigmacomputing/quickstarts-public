# .env configuration file for Sigma Embed JWT QuickStart

# For details on JWT claim requirements, visit:
# https://help.sigmacomputing.com/docs/create-an-embed-api-with-json-web-tokens#jwt-claims

# Required Embed Configuration
BASE_URL=your_sigma_url_to_embed
CLIENT_ID=your_client_id
SECRET=your_secret

# Optional Embed Configuration
SESSION_LENGTH=3600

# User-Specific JWT Claims
EMAIL=your_test_embed_user_email
ACCOUNT_TYPE=your_account_type
TEAM=your_team

# Local Server Configuration
PORT=3000
